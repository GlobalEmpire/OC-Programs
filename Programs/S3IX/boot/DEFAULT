-- bootrd.lua - functions for handling the bootrd

function k_require(path)
	path = path .. '.lua'
	local chunk = load(BOOTRD[path])
	return chunk()
end

-- main.lua - S3IX kernel core

function _main()
	local class = k_require 'lib/vendor/middleclass/middleclass'
	k_require 'lib/role/queue'

	local foo = class('Foo')
	foo:include(Queue)

	foo:put(100)
	print(foo:next())	

end

-- Entry.lua - entry point for the S3IX kernel

